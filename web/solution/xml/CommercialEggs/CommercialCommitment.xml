<fieldList xmlns:xs="http://www.cargas.com/schema/csbFormatXML">
	<section displayName="Commercial Commitment" columnSpan ="3">
		<field fieldName="CommercialMarketID" multilineEdit="true" displayName="Commercial Market" padding="1">
			<lookupList>
				<source sql="exec CommercialMarket_Lookup"/>
			</lookupList>
		</field>
		<field fieldName="CommitmentStatusID" multilineEdit="true" displayName="Commitment Status">
			<lookupList>
				<source sql="exec CommitmentStatus_Lookup"/>
			</lookupList>
		</field>
		<field fieldName="CommitmentDateStart" displayName="Commitment Start Date" />
		<field fieldName="CommitmentDateEnd" displayName="Commitment End Date" />
		<field fieldName="CommitmentQty" displayName="Commitment Qty" />
		<field fieldName="Notes" displayName="Notes" columnSpan="3"/>

	</section>
</fieldList>