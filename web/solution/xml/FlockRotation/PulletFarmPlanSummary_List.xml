    <field fieldName="FlockNumber" displayName="Flock Code" className="{{thisRecord.FarmColor}}" readOnly="true"/>
		<field fieldName="PulletFacility" displayName="Pullet Facility" className="{{thisRecord.FarmColor}}"  readOnly="true"/>
