	<section displayName="Pullet - Move,Wash,Litter,Fog" displayStyle="paging:100">
		<field fieldName="FlockNumber" displayName="Flock Code" className="{{thisRecord.FarmColor}} shrinkFontSize_95 min_width85" readOnly="true"/>
		<field fieldName="PulletFacility" displayName="Pullet Facility" className="{{thisRecord.FarmColor}} shrinkFontSize_95 min_width150"  readOnly="true"/>
		<field fieldName="FemaleBreed" displayName="Female&lt;br/&gt;Breed" className="{{thisRecord.FarmColor}}  shrinkFontSize_95 min_width50"  readOnly="true"/>
		<field fieldName="MaleBreed" displayName="Male&lt;br/&gt;Breed" className="{{thisRecord.FarmColor}} shrinkFontSize_95 min_width50"  readOnly="true"/>
		<field fieldName="Current_PlannedHatchDate" displayName="Planned&lt;br/&gt;Hatch Date" className="{{thisRecord.FarmColor}} shrinkFontSize_95 min_width75"  readOnly="true"/>
		<field fieldName="Current_ActualHatchDate" displayName="Actual&lt;br/&gt;Hatch Date" className="{{thisRecord.FarmColor}} shrinkFontSize_95 min_width75"  readOnly="true"/>
