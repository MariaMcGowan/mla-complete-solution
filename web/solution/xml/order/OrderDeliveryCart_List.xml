    <field fieldName="OrderDeliveryCartID" readOnly="true" displayName="Delete" type="command icon delete" ScreenID="OrderDeliveryCart_Delete" visibleFieldName="OrderDeliveryCartID.toString()!" visibleFieldValue="0"/>
  </section>
</fieldList>