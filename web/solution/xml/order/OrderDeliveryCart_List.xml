    <field fieldName="PlannedQty" multilineEdit="true" displayName="Planned Qty"/>
    <field fieldName="ActualQty" multilineEdit="true" displayName="Actual Qty"/>
    <field fieldName="FlockID" multilineEdit="true" displayName="Flock">
      <lookupList>
        <source sql="Flock_Lookup @includeAll = 0, @includeBlank = 0"/>
