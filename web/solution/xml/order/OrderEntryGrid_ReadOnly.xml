<fieldList xmlns:xs="http://www.cargas.com/schema/csbFormatXML">
	<section displayName="Order">
    <field fieldName="OrderID" displayName="Edit" type="link icon edit" ScreenID="OrderEntryGrid_Edit" hideWhenNull="true"/>
    <field fieldName="OrderNbr" multilineEdit="true" displayName="Order Number"/>    
    <field fieldName="CustomerReferenceNbr" multilineEdit="true" displayName="PO Number" padding="1"/>
		<field fieldName="DeliveryDate" multilineEdit="true" displayName="Delivery Date"/>
    <field fieldName="PlannedSetDate" multilineEdit="true" displayName="Planned Set Date"/>
    <field fieldName="DestinationBuildingID" multilineEdit="true" displayName="Destination Building">
      <lookupList>
        <source sql="execute DestinationBuilding_Lookup"></source>
      </lookupList>
    </field>
    <field fieldName="OrderQty" multilineEdit="true" displayName="Order Quantity"/>
    <field fieldName="FlockList" multilineEdit="true" displayName="Flock(s)"/>
    <field fieldName="ClutchPlannedQty" multilineEdit="true" displayName="Planned Lay Date Quantity"/>
    <field fieldName="OrderStatusID" multilineEdit="true" displayName="Order Status">
      <lookupList>
        <source sql="execute OrderStatus_Lookup"></source>
      </lookupList>
    </field>
  </section>
</fieldList>